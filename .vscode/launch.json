    "version": "0.2.0",
    "configurations": [
        {
            "name": "stlink",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/f3d.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "stlink",
            "preLaunchTask": "build",
            "configFiles": [
                "stlink.cfg",
                "stm32f3.cfg"
            ],
            "svdFile": "${workspaceFolder}/STM32F303.svd",
            "postRestartCommands": [
                "monitor reset halt",
                "monitor reset init"
            ],
        },
        {
            "name": "cmsis-dap",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/f3d.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "preLaunchTask": "build",
            "configFiles": [
                "daplink.cfg",
                "stm32f3.cfg"
            ],
            "svdFile": "${workspaceFolder}/STM32F303.svd",
            "postRestartCommands": [
                "monitor reset halt",
                "monitor reset init"
            ],
        }
    ]
}